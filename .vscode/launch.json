  "version": "0.2.0",
  "configurations": [
    {
      "name": "_root: test current-file/selected-test",
      "type": "node-terminal",
      "request": "launch",
      "command": "sst shell -- vitest run ${file} --testNamePattern='${selectedText}'",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        // "EVENTS_INGEST_TOKEN": "asdfasdf"
      },
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    },
    {
      "name": "_root: test current-file",
      "type": "node-terminal",
      "request": "launch",
      "command": "sst shell -- vitest run ${file}",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "_root: tsx current-file",
      "type": "node-terminal",
      "request": "launch",
      "command": "sst shell -- tsx -r tsconfig-paths/register ${file}",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "_root: python current-file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "ENV": "local",
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "/home/izlobin/miniconda3/envs/pytorch-3.10/bin/python",
      "justMyCode": false
